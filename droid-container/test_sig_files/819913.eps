/NP {newpath} def /S {stroke} def /SD {setdash} def /SRGB {setrgbcolor} def
/M {moveto} def /L {lineto} def /RM {rmoveto} def /RL {rlineto} def
/CPT {currentpoint} def /CLP {closepath} def

/Tmin 0.5 def /Tmax 5.5 def           % range of x-axis variable
/mumin -1 def /mumax 4 def     % range of y-axis variable
/xmax 160 def
/ymax 562 def
/lentick 7 def

/box {NP M CPT 4 1 roll exch L CPT exch pop L
	CPT pop exch L CLP} def
/SW {stringwidth} def
/rshw {dup SW pop -1 mul 0 RM show} def
/cshw {dup SW pop -0.5 mul 0 RM show} def
/subshow {CPT gsave translate 0.75 dup scale (A) SW pop dup
	-0.1 mul exch -0.7 mul M
	show CPT pop 0.75 mul grestore 0 RM} def
/supshow {CPT gsave translate 0.75 dup scale 0 (A) SW pop M
	show CPT pop 0.75 mul grestore 0 RM} def
/tophys {mumin sub mumax mumin sub div ymax mul exch
	Tmin sub Tmax Tmin sub div xmax mul exch} def
/pmt {tophys M} def
/plt {tophys L} def
% usage: (20) 20 tlabel
/tlabel {dup NP mumin pmt 0 lentick RL S
	dup NP mumax pmt 0 lentick -1 mul RL S
	mumin tophys 20 sub M cshw} def
% usage (100) 100 mulabel
/mulabel {dup NP Tmin exch pmt lentick 0 RL S
          dup NP Tmax exch pmt lentick -1 mul 0 RL S
          Tmin exch pmt -5 -5 RM rshw} def
/xlabel	{xmax 2 div -43 M cshw} def
/ylsetup {gsave -35 ymax 2 div translate 90 rotate 0 0 M
        SW pop -2 div 0 RM} def
/ylabel {dup ylsetup show grestore} def
/tenxmulabel {dup NP Tmin exch pmt lentick 0 RL S
          dup NP Tmax exch pmt lentick -1 mul 0 RL S
          Tmin exch pmt -5 -5 RM dup SW pop 0.75 mul
	  (10) SW pop add -1 mul 0 RM (10) show supshow} def
/fssfnt {findfont exch scalefont setfont} def
/Hel {/Helvetica fssfnt} def
/Sym {/Symbol fssfnt} def

/P {12 Hel 0 setgray /lfm exch def /C exch def 
	1 lfm C add 0.05 add pmt show ( C=) show C 10 string cvs show} def
/e 0.01 def
/ebar {/f exch def /x exch def
	x f e add log C add NP pmt -5 0 RM 10 0 RL -5 0 RM
	x f e sub dup 0 le {pop 0.001} if log C add plt -5 0 RM 10 0 RL S
	x f dup 0 le {pop 0.001} if log C add tophys NP 2 0 360 arc fill} def

/B {/e exch def /f5 exch def 5 f5 ebar
    /e exch def /f4 exch def 4 f4 ebar
    /e exch def /f3 exch def 3 f3 ebar
    /e exch def /f2 exch def 2 f2 ebar
    /e exch def /f1 exch def 1 f1 ebar
    NP 1 f1 log C add pmt 2 f2 log C add plt 3 f3 log C add plt
	4 f4 log C add plt 5 f5 log C add plt S} def
/Kcolor {1 0 0 SRGB} def
/Kacolor {0.7 0.4 0 SRGB} def
/Qcolor {0 0.7 0 SRGB} def
/Vcolor {0 0.4 1 SRGB} def
/Wcolor {0.5 0 1 SRGB} def
/K {Kcolor B} def
/A {Kacolor B} def
/Q {Qcolor B} def
/V {Vcolor B} def
/W {Wcolor B} def

